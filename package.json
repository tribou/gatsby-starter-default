{
  "name": "gatsby-starter",
  "description": "Gatsby starter",
  "version": "1.0.0",
  "license": "Apache-2.0",
  "author": "Aaron Tribou <aaron.tribou@gmail.com>",
  "homepage": "https://github.com/rocksauce/procliq-website#readme",
  "repository": {
    "type": "git",
    "url": "https://github.com/tribou/gatsby-starter"
  },
  "keywords": [
    "gatsby"
  ],
  "engines": {
    "node": ">=8.11.4",
    "yarn": ">=1.10.1"
  },
  "dependencies": {
    "gatsby": "^2.0.0",
    "gatsby-plugin-manifest": "^2.0.2",
    "gatsby-plugin-offline": "^2.0.5",
    "gatsby-plugin-react-helmet": "^3.0.0",
    "gatsby-plugin-robots-txt": "^1.3.0",
    "gatsby-plugin-sass": "^2.0.1",
    "gatsby-plugin-sitemap": "^2.0.1",
    "node-sass": "^4.9.3",
    "react": "^16.5.1",
    "react-dom": "^16.5.1",
    "react-helmet": "^5.2.0",
    "sanitize.css": "^8.0.0"
  },
  "devDependencies": {
    "babel-eslint": "^10.0.1",
    "eslint": "^5.6.1",
    "eslint-config-prettier": "^3.1.0",
    "eslint-plugin-tribou": "^2.0.1",
    "husky": "^1.1.1",
    "prettier": "^1.14.2",
    "pretty-quick": "^1.7.0"
  },
  "prettier": {
    "trailingComma": "es5"
  },
  "scripts": {
    "build": "gatsby build",
    "dev": "npm run yarn && gatsby develop -p 3000",
    "lint": "eslint --ignore-path .gitignore src etc",
    "precommit": "pretty-quick --staged",
    "prettier": "prettier '**/*.{js,md,json,css,scss}' --write",
    "start": "npm run --silent dev",
    "start-prod": "npm run --silent build && gatsby serve",
    "test": "echo \"Error: no test specified\" && exit 1",
    "yarn": "npm run --silent yarn-bin -- install --prefer-offline",
    "yarn-bin": "node bin/yarn-1.10.1.js"
  }
}
